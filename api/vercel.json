{
    "version": 2,
    "name": "retail-store-finder-api",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "iad1"
    ],
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}